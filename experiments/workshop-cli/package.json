{
  "name": "cli-in-ts",
  "description": "NodeConf EU 2019 CLI in TypeScript Workshop",
  "version": "1.0.5",
  "author": "Alex Korzhikov @korzio",
  "bin": {
    "workshop": "./bin/run"
  },
  "bugs": "https://github.com/korzio/note/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "cli-ux": "^5.3.3",
    "enquirer": "^2.3.2",
    "marked": "^4.0.10",
    "marked-terminal": "^3.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.4",
    "@types/marked": "^0.6.5",
    "@types/marked-terminal": "^3.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.2",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/assets",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/korzio/note",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "workshop",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/korzio/note/tree/master/experiments/workshop-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
