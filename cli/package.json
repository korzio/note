{
  "name": "root",
  "private": true,
  "devDependencies": {
    "@types/node": "^12.7.5",
    "lerna": "^3.16.4",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "clean": "lerna clean --yes",
    "clean:modules": "rm -rf node_modules",
    "clean:environment": "rm ~/.local/share/@korzio/note/package.json",
    "clean:note": "npm uninstall --global @note-cli/core",
    "install:note": "npm install --global packages/core",
    "lerna:bootstrap": "lerna bootstrap --force-local",
    "postinstall": "npm run lerna:bootstrap; npm run tsc",
    "tsc": "lerna run tsc"
  },
  "dependencies": {}
}
